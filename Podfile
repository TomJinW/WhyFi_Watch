	platform :watchos, '6.0'
    pod 'SwiftyJSON', '~> 4.0'
end

target 'WhyFi_Watch WatchKit Extension' do
	platform :watchos, '6.0'
    pod 'SwiftyJSON', '~> 4.0'
end